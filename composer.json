{
 
    "name": "01mrlast/BlueCMS",
	"description": "BlueCMS - CMS based on Laravel Framework .",
	"license": "MIT",
	"keywords": ["Laravel", "cms", "bluecms"],
	"authors": [
		{
		    "name": "Mr last",
		    "email": "samson@mrlast.com"
		}
	],
"require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.7",
    "intervention/image": "^2.7",
    "laravel/framework": "^12.0",
    "laravel/tinker": "^2.8",
    "laravel/ui": "^4.2",
    "overtrue/laravel-follow": "^5.3",
    "overtrue/laravel-like": "^5.4"
},
"require-dev": {
    "spatie/laravel-ignition": "^2.0",
    "fzaninotto/faker": "^1.9.2",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^10.4"
}
,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
     "name": "mrlast/bluecms",
    "type": "framework",
    "description": "BlueCMS - The Laravel Framework CMS.",
    "keywords": ["CMS","laravel"],
    "homepage": "https://github.com/01mrlast/BlueCMS/",
    "authors": [
        {
            "name": "Samson last",
            "email": "samson@mrlast.com",
            "homepage": "http://mrlast.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
